@model PruebaPasantia.Models.Auto

@{
    ViewData["Title"] = "Detalles";
}

<h1>Detalles</h1>

<div>
    <h4>Auto</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Patente)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Patente)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Marca)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Marca)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Modelo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Modelo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Año)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Año)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Kms)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Kms)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Foto1)
        </dt>
        <dd class="col-sm-10">
            @if (Model.Foto1 != "")
            {
                <img width="64" src="@Model.Foto1" /> }
            else
            {
                <p>Sin Foto</p>}
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Foto2)
        </dt>
        <dd class="col-sm-10">
            @if (Model.Foto2 != "")
            {
                <img width="64" src="@Model.Foto2" /> }
            else
            {
                <p>Sin Foto</p>}
            </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Editar", "Edit", new { id = @Model.IdAuto }) |
    <a asp-action="Index">Volver a la lista</a>
</div>
